<div class="mdl-card mdl-shadow--2dp">
	<table class="risks-list-table mdl-data-table mdl-js-data-table mdl-shadow--2dp">
		<thead>
			<tr>
				<!-- ko foreach: headers-->
				<th data-bind="click: $parent.sort">
					<span data-bind="text: title"></span>
					<img src="~/images/down.png" data-bind="visible: sorted" />
				</th>
				<!-- /ko -->
			</tr>
		</thead>
		<tbody>
			<!-- ko foreach: risks -->
			<tr data-bind="click: $parent.edit">
				<th data-bind="text: riskId"></th>
				<th data-bind="text: descripton" class="left"></th>
				<th data-bind="text: mitigation" class="left"></th>
				<th data-bind="text: dateRaised"></th>
				<th data-bind="text: preRiskLevel, css: preRiskLevel"></th> @*this needs to change*@
			</tr>
			<!-- /ko -->
		</tbody>
	</table>
</div>